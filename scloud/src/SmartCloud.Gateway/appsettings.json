{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "MQTT": {
    "BrokerHost": "localhost",
    "BrokerPort": "1883",
    "Username": "",
    "Password": ""
  },
  "InfluxDB": {
    "Url": "http://localhost:8086",
    "Token": "your-influxdb-token",
    "Bucket": "smartcloud",
    "Organization": "smartcloud-org"
  },
  "Dashboard": {
    "Url": "http://localhost:57822"
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    }
  }
}
